datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Message {
  content   String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  sender    String?
  status    String?
  updatedAt DateTime @updatedAt
}
